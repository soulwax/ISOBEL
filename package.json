{
  "name": "isobel",
  "version": "2.15.0",
  "description": "ðŸŽ§ a self-hosted Discord music bot that doesn't suck ",
  "repository": "git@github.com:soulwax/ISOBEL.git",
  "author": "Max Isom <hi@maxisom.me>",
  "license": "MIT",
  "private": true,
  "types": "dts/types",
  "type": "module",
  "engines": {
    "node": ">=20.0.0"
  },
  "files": [
    "src"
  ],
  "scripts": {
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "lint:fix": "eslint \"src/**/*.{ts,tsx}\" --fix",
    "typecheck": "tsc --noEmit",
    "test": "npm run lint",
    "start": "npm run env:set-database-url -- tsx src/scripts/migrate-and-start.ts",
    "cache:clear-key-value": "npm run env:set-database-url tsx src/scripts/cache-clear-key-value.ts",
    "dev": "npm run env:set-database-url -- tsx watch src/scripts/start.ts",
    "debug": "pm2 delete isobel || true; npm run dev",
    "migrations:generate": "npm run prisma:with-env migrate dev",
    "migrations:run": "npm run prisma:with-env migrate deploy",
    "prisma:with-env": "npm run env:set-database-url prisma",
    "prisma:generate": "prisma generate",
    "env:set-database-url": "tsx src/scripts/run-with-database-url.ts",
    "release": "release-it",
    "build": "tsc",
    "postbuild": "pm2 startOrReload ecosystem.config.cjs --env production || pm2 start ecosystem.config.cjs --env production",
    "pm2:start": "pm2 start ecosystem.config.cjs --env production",
    "pm2:start:prod": "pm2 start ecosystem.config.cjs --env production",
    "pm2:start:staging": "pm2 start ecosystem.config.cjs --env staging",
    "pm2:start:dev": "pm2 start ecosystem.config.cjs --env development",
    "pm2:stop": "pm2 stop isobel",
    "pm2:stop:all": "pm2 stop all",
    "pm2:restart": "pm2 restart isobel",
    "pm2:restart:prod": "pm2 restart ecosystem.config.cjs --env production",
    "pm2:restart:staging": "pm2 restart ecosystem.config.cjs --env staging",
    "pm2:restart:dev": "pm2 restart ecosystem.config.cjs --env development",
    "pm2:reload": "pm2 reload isobel",
    "pm2:delete": "pm2 delete isobel",
    "pm2:delete:all": "pm2 delete all",
    "pm2:reset": "pm2 delete all && pm2 kill",
    "pm2:logs": "pm2 logs isobel",
    "pm2:logs:error": "pm2 logs isobel --err",
    "pm2:logs:out": "pm2 logs isobel --out",
    "pm2:logs:lines": "pm2 logs echo --lines 100",
    "pm2:logs:flush": "pm2 flush",
    "pm2:status": "pm2 status",
    "pm2:info": "pm2 describe isobel",
    "pm2:monit": "pm2 monit",
    "pm2:list": "pm2 list",
    "pm2:save": "pm2 save",
    "pm2:resurrect": "pm2 resurrect",
    "pm2:startup": "pm2 startup",
    "pm2:unstartup": "pm2 unstartup",
    "pm2:update": "pm2 update",
    "pm2:kill": "pm2 kill",
    "pm2:version": "pm2 --version",
    "postinstall": "patch-package",
    "logs:all": "pm2 logs"
  },
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "@release-it/keep-a-changelog": "^2.5.0",
    "@types/bluebird": "^3.5.42",
    "@types/debug": "^4.1.12",
    "@types/fluent-ffmpeg": "^2.1.28",
    "@types/fs-capacitor": "^2.0.0",
    "@types/ms": "2.1.0",
    "@types/node": "^20.19.25",
    "@types/node-emoji": "^2.1.0",
    "@types/spotify-web-api-node": "^5.0.11",
    "@types/validator": "^13.15.10",
    "@types/ws": "8.18.1",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^9.39.1",
    "eslint-config-xo-typescript": "^9.0.0",
    "eslint-plugin-import": "^2.32.0",
    "husky": "^9.1.7",
    "pm2": "^6.0.14",
    "prisma": "^5.22.0",
    "release-it": "^19.0.6",
    "type-fest": "^2.19.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.48.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test"
    }
  },
  "dependencies": {
    "@discordjs/builders": "^1.13.1",
    "@discordjs/opus": "^0.10.0",
    "@discordjs/rest": "^2.6.0",
    "@discordjs/voice": "^0.19.0",
    "@distube/ytdl-core": "^4.16.12",
    "@distube/ytsr": "^2.0.4",
    "@prisma/client": "^5.22.0",
    "@snazzah/davey": "^0.1.8",
    "@types/libsodium-wrappers": "^0.7.14",
    "array-shuffle": "^4.0.0",
    "debug": "^4.4.3",
    "delay": "^7.0.0",
    "discord-api-types": "^0.38.36",
    "discord.js": "^14.25.1",
    "dotenv": "^16.6.1",
    "execa": "^6.1.0",
    "fluent-ffmpeg": "^2.1.3",
    "fs-capacitor": "^7.0.1",
    "get-youtube-id": "^1.0.1",
    "got": "^14.6.5",
    "hasha": "^7.0.0",
    "inversify": "^7.10.4",
    "iso8601-duration": "^2.1.3",
    "libsodium-wrappers": "^0.7.15",
    "make-dir": "^3.1.0",
    "node-emoji": "^2.2.0",
    "nodesplash": "^0.1.1",
    "ora": "^9.0.0",
    "p-event": "^7.0.1",
    "p-limit": "^7.2.0",
    "p-queue": "^9.0.1",
    "p-retry": "6.2.0",
    "pagination.djs": "^4.0.18",
    "parse-duration": "^2.1.4",
    "patch-package": "^8.0.1",
    "postinstall-postinstall": "^2.1.0",
    "read-pkg": "^10.0.0",
    "reflect-metadata": "^0.2.2",
    "sponsorblock-api": "^0.2.4",
    "spotify-uri": "^3.0.4",
    "spotify-web-api-node": "^5.0.2",
    "sync-fetch": "^0.3.1",
    "tsx": "^4.21.0",
    "xbytes": "^1.9.1",
    "ytsr": "^3.8.4"
  },
  "resolutions": {
    "@types/ws": "8.18.1"
  }
}
