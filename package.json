{
  "name": "isobel",
  "version": "3.1.0",
  "description": "ðŸŽ§ a self-hosted Discord music bot that doesn't suck ",
  "repository": "git@github.com:soulwax/ISOBEL.git",
  "author": "Max Isom <hi@maxisom.me>, Christian Kling <soulwax@madtec.org>",
  "license": "GPL-3.0-only",
  "private": true,
  "types": "dts/types",
  "type": "module",
  "engines": {
    "node": ">=24.0.0"
  },
  "files": [
    "src"
  ],
  "scripts": {
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "lint:fix": "eslint \"src/**/*.{ts,tsx}\" --fix",
    "typecheck": "tsc --noEmit",
    "test:unit": "node --import tsx --test \"test/**/*.test.ts\"",
    "test": "pnpm run lint",
    "start": "pnpm run prisma:generate && pnpm run env:set-database-url -- tsx src/scripts/migrate-and-start.ts",
    "cache:clear-key-value": "pnpm run env:set-database-url tsx src/scripts/cache-clear-key-value.ts",
    "dev": "pnpm run prisma:generate && pnpm run env:set-database-url -- tsx watch src/scripts/start.ts",
    "dev:all": "concurrently \"pnpm run dev\" \"pnpm run web:dev:all\" --names \"bot,web\" --prefix-colors \"blue,cyan\"",
    "debug": "pm2 delete isobel || true; pnpm run dev",
    "prisma:with-env": "pnpm run env:set-database-url -- prisma",
    "prisma:generate": "pnpm run prisma:with-env -- generate",
    "prisma:migrate:dev": "pnpm run prisma:with-env -- migrate dev",
    "prisma:migrate:deploy": "pnpm run prisma:with-env -- migrate deploy",
    "prisma:migrate:status": "pnpm run prisma:with-env -- migrate status",
    "env:set-database-url": "tsx src/scripts/run-with-database-url.ts",
    "release": "release-it",
    "build": "tsc",
    "build:all": "pnpm run build && pnpm run web:build",
    "verify:env": "tsx src/scripts/verify-environment.ts",
    "verify:db": "tsx src/scripts/verify-database.ts",
    "deploy": "pnpm run build:all && pnpm run start:all:prod",
    "deploy:dev": "pnpm run build && pnpm run pm2:start:dev && pnpm run web:pm2:start:dev",
    "db:status": "pnpm run prisma:migrate:status",
    "db:reset": "pnpm run prisma:with-env -- migrate reset",
    "health": "curl -s http://localhost:3002/health || echo 'Health check failed - bot may not be running'",
    "health:web": "curl -s http://localhost:3001/health || echo 'Health check failed - web may not be running'",
    "pm2:start": "pm2 start ecosystem.config.cjs --env production",
    "pm2:start:staging": "pm2 start ecosystem.config.cjs --env staging",
    "pm2:start:dev": "pm2 start ecosystem.config.cjs --env development",
    "pm2:stop": "pm2 stop isobel",
    "pm2:stop:all": "pm2 stop all",
    "pm2:restart": "pm2 restart ecosystem.config.cjs --env production --update-env",
    "pm2:restart:staging": "pm2 restart ecosystem.config.cjs --env staging --update-env",
    "pm2:restart:dev": "pm2 restart ecosystem.config.cjs --env development --update-env",
    "pm2:reload": "pm2 reload isobel",
    "pm2:delete": "pm2 delete isobel",
    "pm2:delete:all": "pm2 delete all",
    "pm2:reset": "pm2 delete all && pm2 kill",
    "pm2:logs": "pm2 logs isobel",
    "pm2:logs:error": "pm2 logs isobel --err",
    "pm2:logs:out": "pm2 logs isobel --out",
    "pm2:logs:lines": "pm2 logs echo --lines 100",
    "pm2:logs:flush": "pm2 flush",
    "pm2:status": "pm2 status",
    "pm2:info": "pm2 describe isobel",
    "pm2:monit": "pm2 monit",
    "pm2:list": "pm2 list",
    "pm2:save": "pm2 save",
    "pm2:resurrect": "pm2 resurrect",
    "pm2:startup": "pm2 startup",
    "pm2:unstartup": "pm2 unstartup",
    "pm2:update": "pm2 update",
    "pm2:kill": "pm2 kill",
    "pm2:version": "pm2 --version",
    "postinstall": "patch-package",
    "install:all": "pnpm install -r",
    "install:web": "pnpm --filter isobel-web install",
    "web:dev": "pnpm --filter isobel-web run dev",
    "web:dev:all": "pnpm --filter isobel-web run dev:all",
    "web:build": "pnpm --filter isobel-web run build",
    "web:lint": "pnpm --filter isobel-web run lint",
    "web:preview": "pnpm --filter isobel-web run preview",
    "web:pm2:start": "pnpm --filter isobel-web run pm2:start",
    "web:pm2:start:dev": "pnpm --filter isobel-web run pm2:start:dev",
    "web:pm2:start:prod": "pnpm --filter isobel-web run pm2:start:prod",
    "web:pm2:stop": "pnpm --filter isobel-web run pm2:stop",
    "web:pm2:restart": "pnpm --filter isobel-web run pm2:restart",
    "web:pm2:delete": "pnpm --filter isobel-web run pm2:delete",
    "web:pm2:logs": "pnpm --filter isobel-web run pm2:logs",
    "web:pm2:logs:web": "pnpm --filter isobel-web run pm2:logs:web",
    "web:pm2:status": "pnpm --filter isobel-web run pm2:status",
    "lint:all": "pnpm run lint && pnpm --filter isobel-web run lint",
    "lint:fix:all": "pnpm run lint:fix && pnpm --filter isobel-web run lint -- --fix",
    "typecheck:all": "pnpm run typecheck && pnpm --filter isobel-web exec tsc --noEmit",
    "start:all": "pnpm run pm2:start:prod && pnpm run web:pm2:start",
    "start:all:dev": "pnpm run pm2:start:dev && pnpm run web:pm2:start:dev",
    "start:all:prod": "pnpm run pm2:start:prod && pnpm run web:pm2:start:prod",
    "stop:all": "pnpm run pm2:stop && pnpm run web:pm2:stop",
    "restart:all": "pnpm run pm2:restart && pnpm run web:pm2:restart",
    "logs:all": "pm2 logs"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@release-it/keep-a-changelog": "^7.0.0",
    "@types/bluebird": "^3.5.42",
    "@types/debug": "^4.1.12",
    "@types/ms": "2.1.0",
    "@types/node": "25.3.0",
    "@types/pg": "^8.16.0",
    "@types/spotify-web-api-node": "^5.0.11",
    "@types/validator": "^13.15.10",
    "@types/ws": "8.18.1",
    "@typescript-eslint/eslint-plugin": "^8.53.1",
    "@typescript-eslint/parser": "^8.53.1",
    "concurrently": "^9.2.1",
    "eslint": "^9.39.2",
    "eslint-config-xo-typescript": "^9.0.0",
    "eslint-plugin-import": "^2.32.0",
    "husky": "^9.1.7",
    "pm2": "^6.0.14",
    "prisma": "^7.3.0",
    "release-it": "^19.2.4",
    "type-fest": "^5.4.3",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.54.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pnpm test"
    }
  },
  "dependencies": {
    "@discordjs/builders": "^1.13.1",
    "@discordjs/opus": "^0.10.0",
    "@discordjs/rest": "^2.6.0",
    "@discordjs/voice": "^0.19.0",
    "@distube/ytdl-core": "^4.16.12",
    "@distube/ytsr": "^2.0.4",
    "@prisma/adapter-pg": "^7.3.0",
    "@prisma/client": "^7.3.0",
    "@snazzah/davey": "^0.1.9",
    "@types/libsodium-wrappers": "^0.7.14",
    "array-shuffle": "^4.1.0",
    "debug": "^4.4.3",
    "delay": "^7.0.0",
    "discord-api-types": "^0.38.38",
    "discord.js": "^14.25.1",
    "dotenv": "^17.2.4",
    "execa": "^9.6.1",
    "ffmpeggy": "^2.1.8",
    "fs-capacitor": "^8.0.0",
    "get-youtube-id": "^1.0.1",
    "got": "^14.6.6",
    "hasha": "^7.0.0",
    "inversify": "^7.11.0",
    "iso8601-duration": "^2.1.3",
    "libsodium-wrappers": "^0.8.2",
    "make-dir": "^5.1.0",
    "node-emoji": "^2.2.0",
    "nodesplash": "^0.1.1",
    "ora": "^9.3.0",
    "p-event": "^7.1.0",
    "p-limit": "^7.3.0",
    "p-queue": "^9.1.0",
    "p-retry": "7.1.1",
    "pagination.djs": "^4.0.18",
    "parse-duration": "^2.1.5",
    "patch-package": "^8.0.1",
    "pg": "^8.18.0",
    "read-pkg": "^10.0.0",
    "reflect-metadata": "^0.2.2",
    "sponsorblock-api": "^0.2.4",
    "spotify-uri": "^4.1.0",
    "spotify-web-api-node": "^5.0.2",
    "sync-fetch": "^0.6.0",
    "tsx": "^4.21.0",
    "xbytes": "^1.9.1"
  },
  "resolutions": {
    "@types/ws": "8.18.1"
  },
  "packageManager": "pnpm@10.30.2"
}
